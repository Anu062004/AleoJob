{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist"
            }
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "https://aleojob.onrender.com/api/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ],
    "env": {
        "NEXT_PUBLIC_BACKEND_URL": "https://aleojob.onrender.com",
        "NEXT_PUBLIC_ALEO_NETWORK": "testnet",
        "NEXT_PUBLIC_ALEO_ENDPOINT": "https://api.provable.com/v2/testnet",
        "NEXT_PUBLIC_ALEO_RPC_URL": "https://api.provable.com/v2/testnet"
    }
}